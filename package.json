{
  "name": "electron-react",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/electron.js",
  "scripts": {
    "start": "concurrently \"npm run start:render\" \"wait-on http://127.0.0.1:6001 && npm run start:main\"",
    "start:render": "webpack-dev-server --config ./webpack/webpack.render.dev.js",
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/electron.js",
    "build": "npm run build:main && npm run build:render",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "build:render": "webpack --config ./webpack/webpack.render.prod.js",
    "ele:pack": "electron-builder --dir && npm run move:viz",
    "ele:dist": "electron-builder",
    "move:viz": "node scripts/move-viz.js"
  },
  "build": {
    "appId": "appid20210819",
    "productName": "ElectronApp",
    "copyright": "Copyright Â© 2021 ${author}",
    "files": [
      "dist/",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "distAssets"
    }
  },
  "keywords": [],
  "author": "YKN",
  "license": "ISC",
